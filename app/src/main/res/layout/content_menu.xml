<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/tw__solid_white"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.project.ta.findoctor.Activity.MenuActivity"
    tools:showIn="@layout/app_bar_menu">

    <ViewStub
        android:id="@+id/dokter_stub"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/inflateddokter"
        android:layout="@layout/stub_dokter" />
    <ViewStub
        android:id="@+id/pasien_stub"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/inflatedpasien"
        android:layout="@layout/stub_pasien" />
    <ViewStub
        android:id="@+id/asisten_stub"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/inflatedasisten"
        android:layout="@layout/stub_asisten" />
    <ViewStub
        android:id="@+id/guest_stub"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inflatedId="@+id/inflatedguest"
        android:layout="@layout/stub_guest" />
</RelativeLayout>
